{
  "name": "jarvis-obsidian-sync",
  "version": "1.0.0",
  "description": "Companion Obsidian plugin for Jarvis Bot - sync transcribed WhatsApp voice notes to your vault",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "jarvis-bot",
    "whatsapp",
    "voice",
    "transcription",
    "sync",
    "ai",
    "notes"
  ],
  "author": "Thalys Guimar√£es",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
